<!-- BEGIN: Subheader -->
<div class="m-subheader ">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h3 class="m-subheader__title m-subheader__title--separator">Customer Dashboard</h3>

        </div>
        <span class="font-medium-2 mr-2">Filter : <span></span> </span>
        <div>
            <select class="form-control m-input" id="SelectCount">
                <option value="1">Today</option>
                <option value="2">Yesterday</option>
                <option value="3">This Week</option>
                <option value="4">Previous Week</option>
                <option value="5">This Month</option>
                <option value="6">Previous Month</option>
                <option value="7">LifeTime</option>

            </select>
        </div>
        <label class="col-md-2 col-sm-12 col-12 col-form-label" hidden>Select Hub</label>
        <div class="col-md-3 col-sm-3 col-6" hidden>
            <select class="form-control m-input m-input--solid" id="Hub" name="Hub"hidden>
                <option value="0" selected="" disabled="">Select Hub</option>
                @foreach (var hublist in Model.getHublist)
                {
                    <option value="@hublist.HubId">@hublist.HubName -@hublist.Area</option>
                }
            </select>
            <span class="text-danger font-small-3 text-bold-600" style="display:none;"></span>
        </div>
        <label class="col-md-2 col-sm-12 col-12 col-form-label" hidden>Select ZipCode</label>
        <div class="col-md-2 col-sm-3 col-6" hidden>
            <select class="form-control m-input m-input--solid" id="ZipCode" name="ZipCode" hidden>
                <option value="0" selected="" disabled>Select ZipCode</option>
                @foreach (var ziplist in Model.getZiplist)
                {
                    <option value="@ziplist.ZipCode">@ziplist.ZipCode</option>
                }
            </select>
            <span class="text-danger font-small-3 text-bold-600" style="display:none;"></span>
        </div>
    </div>
</div>
<!-- END: Subheader -->


