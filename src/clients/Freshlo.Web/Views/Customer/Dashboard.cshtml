                                    <tr>
                                        <th>Zip Code</th>
                                        <th>Total Customers</th>

                                    </tr>
                                </thead>
                                <tbody id="saleslist">
                                    <tr id="">
                                        <td class="font-medium-4">
                                            400706
                                        </td>
                                        <td class="font-medium-4">23423</td>

                                    </tr>
                                    <tr id="">
                                        <td class="font-medium-4">
                                            400706
                                        </td>
                                        <td class="font-medium-4">23423</td>

