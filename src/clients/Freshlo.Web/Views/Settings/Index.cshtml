                                                    <td><a href="#" class="text-danger deletebrandId" data-id="@item.Id"> <i class="flaticon-delete deleteId"> </i></a></td>
                                                </tr>

                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane" id="table" role="tabpanel">
                    <div class="m-portlet">
                        <div class="m-portlet__head">
                            <div class="m-portlet__head-caption">
                                <div class="m-portlet__head-title">
                                    <span class="m-portlet__head-icon m--hide">
                                        <i class="la la-gear"></i>
                                    </span>
                                    <h3 class="m-portlet__head-text">
                                        Manage Table
                                    </h3>
                                </div>
                            </div>
                            <div class="m-portlet__head-tools">
                                <button type="button" class="btn btn-dark pull-right newTable" data-toggle="modal" data-target="#NewTable"><i class="la la-plus"></i> New Table</button>
                            </div>
                        </div>
                        <div class="m-portlet__body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped-even ab-table-head-bg-warning-light table-borderless table-middle">
                                            <thead>
                                                <tr>
                                                    <th>Table Id</th>
                                                    <th>Table Name</th>
                                                    <th>Branch</th>
                                                    <th>Created On / By</th>
                                                    <th>Edit</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.Gettablelist)
