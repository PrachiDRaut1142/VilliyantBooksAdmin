                </div>
                <div class="tab-pane" id="slots" role="tabpanel">
                    <div class="m-portlet">
                        <div class="m-portlet__head">
                            <div class="m-portlet__head-caption">
                                <div class="m-portlet__head-title">
                                    <span class="m-portlet__head-icon m--hide">
                                        <i class="la la-gear"></i>
                                    </span>
                                    <h3 class="m-portlet__head-text">
                                        Manage Delivery Slots
                                    </h3>
                                </div>
                            </div>
                        </div>
                        <div class="m-portlet__body">
                            <div class="row">
                                <div class="col-md-12">
                                    <form class="m-form m-form--fit m-form--label-align-right" enctype="multipart/form-data" asp-action="Index" asp-controller="Settings" id="SlotCreate" method="post">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <select class="form-control m-input m-input--solid SlotType" id="SlotType" name="SlotType">
                                                        <option value="-1">Select Slot</option>
                                                        <option value="0">Standrad</option>
                                                        <option value="1">Express</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <input type="text" class="form-control m-input m-input--solid" placeholder="Enter Timing" id="Timing" name="Timing">
                                                </div>
                                            </div>
                                            <div class="col-md-1">
                                                <div class="form-group">
                                                    <button type="submit" class="btn btn-success" id="CreateSlot"><i class="la la-plus"></i> ADD NEW</button>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
