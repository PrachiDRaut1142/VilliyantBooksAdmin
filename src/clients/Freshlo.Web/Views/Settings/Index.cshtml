                                <button type="button" class="btn btn-dark pull-right newbrand" data-toggle="modal" data-target="#NewBrand"><i class="la la-plus"></i> New Brand</button>
                            </div>
                        </div>
                        <div class="m-portlet__body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped-even ab-table-head-bg-warning-light table-borderless table-middle">
                                            <thead>
                                                <tr>
                                                    <th>Brand Name</th>
                                                    <th>Brand Logo</th>
                                                    <th>Total Item</th>
                                                    <th>Created On / By</th>
                                                    <th>Edit</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.Getbrandlist)
                                                {

                                                <tr>
                                                    <td>
                                                        <a href="#" class="text-link">@item.BrandName</a>
                                                    </td>
                                                    <td>
                                                        <img alt="Brand Image" src="https://freshlo.oss-ap-south-1.aliyuncs.com/HurTex/brand/@(item.BrandId).png" onerror="this.onerror = null;
