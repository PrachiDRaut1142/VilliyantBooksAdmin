                                                <tr>
                                                    <th>Timing</th>
                                                    <th>SlotTypeDesc</th>
                                                    <th>Edit</th>
                                                    <th>Remove</th>

                                                </tr>
                                            </thead>
                                            <tbody>

                                                @foreach (var item in Model.GetSlotlist)
                                                {
                                                    <tr>
                                                        <td>@item.Timing </td>
                                                        <td>@item.SlotTypeDesc</td>
                                                        <td><a href="#" data-toggle="modal" data-target="#Slots" class="text-dark EditSlot" data-id="@item.Id"> <i class="flaticon-edit"> </i> </a></td>
                                                        <td><a href="#" class="text-danger deleteSlotId" data-id="@item.Id"> <i class="flaticon-delete deleteSlotId"> </i> </a></td>

                                                    </tr>

                                                }


                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane" id="brand" role="tabpanel">
                    <div class="m-portlet">
                        <div class="m-portlet__head">
                            <div class="m-portlet__head-caption">
                                <div class="m-portlet__head-title">
                                    <span class="m-portlet__head-icon m--hide">
                                        <i class="la la-gear"></i>
                                    </span>
                                    <h3 class="m-portlet__head-text">
                                        Manage Brands
                                    </h3>
                                </div>
                            </div>
