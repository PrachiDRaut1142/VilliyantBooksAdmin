@using Freshlo.Web.Models.ItemMaster;
@using System.Globalization;
@model ItemMasterVM;
@{
    var a = 1;
    ViewData["Title"] = "HubPriceDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var offer = Model.offerType.Where(q => q.Value == Model.ItemMastersDetail.offer_type).Select(q => q.Text).FirstOrDefault();
    var aliyunpath = Model.businessInfo.aliyunPath;
}


<style>
    .heading-style {
        font-size: 1.2rem;
        color: #464457;
    }

    .redborder {
        border-color: #ff5454;
    }

    .recommended-header {
        margin: 0 0 1rem 0;
        padding: 0.8rem 0.5rem;
        background-color: #fbf6e5;
    }
</style>

<!-- BEGIN: Subheader -->
<div class="m-subheader ">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h3 class="m-subheader__title m-subheader__title--separator">Item Management</h3>
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item m-nav__item--home">
                    <a href="/Sale/tablestate/" class="m-nav__link m-nav__link--icon">
                        <i class="m-nav__link-icon la la-home"></i>
                    </a>
                </li>
                <li class="m-nav__separator">-</li>
                <li class="m-nav__item">
                    <a href="/Sale/tablestate/" class="m-nav__link">
                        <span class="m-nav__link-text">Home</span>
                    </a>
                </li>
                <li class="m-nav__separator">-</li>
                <li class="m-nav__item">
                    <a href="/ItemMaster/Manage/" class="m-nav__link">
                        <span class="m-nav__link-text">Item Manage</span>
                    </a>
                </li>
                <li class="m-nav__separator">-</li>
                <li class="m-nav__item">
                    <a href="javascripts:;" class="m-nav__link">
                        <span class="m-nav__link-text">Item Detail</span>
                    </a>
                </li>
            </ul>
        </div>
        <div>
            <div class="d-inline-flex">
                <select asp-for="ItemMastersDetail.Approval" class="form-control mr-2" id="ApprovalId" name="ApprovalId">
                    <option value="Pending">Pending</option>
                    <option value="Approved">Approved</option>
                </select>
                <button type="button" id="btnUpdateItem" class="btn btn-danger mr-2" hidden><i class="fa fa-save"> </i> Update Changes</button>
            </div>
        </div>
    </div>
</div>
<!-- END: Subheader -->

<div class="m-content">
    <form class="m-form m-form--fit m-form--label-align-right" enctype="multipart/form-data" action="/ItemMaster/Detail/" id="itemMasterformid" method="post">
        <input type="text" id="Aliyunkey" name="Aliyunkey" value="@aliyunpath" hidden />
        <div class="row">
            <div class="col-md-3">
                <!--begin:: Widgets/Blog-->
                <div class="m-portlet m-portlet--bordered-semi m-portlet--rounded-force">
                    <div class="m-portlet__head m-portlet__head--fit" style="background-color: #f2f3f863;">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-action float-right">
                                <button type="button" id="DefaultImages" class="btn btn-sm m-btn--pill btn-primary" hidden>Set default</button>
                                <div>
                                    <button type="button" id="RemoveImg" class="btn btn-sm m-btn--pill btn-danger" style="display:none" hidden>Remove</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="m-widget19">
                                    <div class="m-widget19__pic m-portlet-fit--top m-portlet-fit--sides">
                                        <img id="myImage" src="https://freshlo.oss-ap-south-1.aliyuncs.com/@aliyunpath/icon/@(Model.ItemMastersDetail.ItemId).png" alt="" style="height:300px;">
                                        <h3 class="m-widget19__title m--font-light">
                                            @Model.ItemMastersDetail.PluName
                                        </h3>
                                        <div class="m-widget19__shadow"></div>
                                    </div>
                                    <div class="m-widget19__content">
                                        <div class="m-widget19__header">
                                            <div id="appImage" class="">
                                                <a href="#" class="PictureImg" data-id="1">
                                                    <img id="galleryid_1" src="https://freshlo.oss-ap-south-1.aliyuncs.com/@aliyunpath/icon/@(Model.ItemMastersDetail.ItemId).png" class="img-thumb-df" style="width: 50px; height: 50px;" />
                                                    <span hidden id="SpnId_1">https://freshlo.oss-ap-south-1.aliyuncs.com/@aliyunpath/icon/@(Model.ItemMastersDetail.ItemId).png</span>
                                                </a>
                                                <input type="hidden" id="DefaultImg_1" value="@(Model.ItemMastersDetail.ItemId)" />
                                                @foreach (var img in Model.GetGalleryImage)
                                                {
                                                    a++;
                                                    <a href="#" class="PictureImg" data-id="@a">
                                                        <img id="galleryid_@a" src="https://freshlo.oss-ap-south-1.aliyuncs.com/@img" class="img-thumb-df gallaryimg" style="width: 50px; height: 50px;" />
                                                        <span hidden id="SpnId_@a">https://freshlo.oss-ap-south-1.aliyuncs.com/@img</span>
                                                    </a>
                                                    <input type="hidden" id="DefaultImg_@a" value="@(img)" />

                                                }
                                            </div>
                                            <span id="ImagesCount" hidden>@a</span>
                                            <a href="#" title="Upload Image">
                                                <span style="opacity:0.4" onclick="$('#Useravatar').click();"><i class="fa fa-cart-arrow-down font-large-1"></i></span>
                                                <input id="Useravatar" type="file" name="files" multiple="" style="visibility: hidden" onchange="PreviewImage();">
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div class="">
                            <div class="row">
                                <div class="col-md-6">
                                    <label>Featured</label>
                                    <div>
                                        <label class="ab-switch mt-2">
                                            <input type="checkbox" id="FeaturedCheckbox">
                                            <span class="slider round"></span>
                                        </label>
                                        <input type="hidden" id="featuretest" name="feature" asp-for="ItemMastersDetail.featured" />
                                        <input type="hidden" id="Approval" name="Approval" value="@Model.ItemMastersDetail.Approval" />

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <label>Coupon Allowed</label>
                                    <div>
                                        <label class="ab-switch mt-2">
                                            <input type="checkbox" id="CoupenCheckbox">
                                            <span class="slider round"></span>
                                        </label>
                                        <input type="hidden" id="Coupentest" name="Coupen" asp-for="ItemMastersDetail.Coupen" />
                                    </div>
                                </div>

                            </div>
                            <div class="row mt-3">
                                <div class="col-md-6">
                                    <label>Availability</label>
                                    <div>
                                        <label class="ab-switch mt-2">
                                            <input type="checkbox" id="AvailabilityCheckbox">
                                            <span class="slider round"></span>
                                        </label>
                                        <input type="hidden" id="avaliblitytest" name="seasonSale" asp-for="ItemMastersDetail.seasonSale" />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <label>Chef's Special</label>
                                    <div>
                                        <label class="ab-switch mt-2">
                                            <input type="checkbox" id="ChckSpecialCheckbox">
                                            <span class="slider round"></span>
                                        </label>
                                        <input type="hidden" id="checksptest" name="Check_Speical" asp-for="ItemMastersDetail.Check_Speical" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div class="row">
                            <div class="col-md-6">
                                <label>Product Code</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.PluCode</h6>
                            </div>
                            <div class="col-md-6">
                                <label>Item Type</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.ItemType</h6>
                            </div>
                        </div>
                        <hr />
                        <div class="row">
                            <div class="col-md-6">
                                <label>Offered As?</label>
                                <h6 class="font-medium-1 text-bold-600">@(offer)</h6>
                            </div>
                            <div class="col-md-6">
                                <label>Item Tag</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.Tag</h6>
                            </div>
                        </div>
                        <hr />
                        <div class="row" style="display:none">
                            <div class="col-md-6">
                                <label>Purchase Price</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.Purchaseprice</h6>
                            </div>
                            <div class="col-md-6">
                                <label>Wastage %</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.Wastage %</h6>
                            </div>
                        </div>
                        <div class="row" style="display:none">
                            <div class="col-md-6">
                                <label>Selling Price</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.SellingPrice</h6>
                            </div>
                            <div class="col-md-6">
                                <label>Market Price</label>
                                <h6 class="font-medium-1 text-bold-600">@Model.ItemMastersDetail.MarketPrice</h6>
                            </div>
                        </div>
                        <hr />
                        <div class="row" style="display:none">
                            <div class="col-md-12">
                                <label>Updated On</label>
                                <h6 class="font-medium-1 text-bold-600">@(Model.ItemMastersDetail.LastUpdatedOn.ToString(" dd-MMM-yyyy hh:mm tt", CultureInfo.InvariantCulture))</h6>
                            </div>
                            <div class="col-md-12">
                                <label>Updated By</label>
                                <h6 class="font-medium-1 text-bold-600">@(Model.ItemMastersDetail.CreatedName)</h6>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end:: Widgets/Blog-->
            </div>
            <div class="col-md-9">
                <div class="row">
                    <div class="col-md-12">
                        <!--begin::Portlet-->
                        <div class="m-portlet m-portlet--tabs">
                            <ul class="nav nav-tabs  m-tabs-line m-nav-tab-account-lookup" role="tablist">
                                <li class="nav-item m-tabs__item" style="display:none">
                                    <a class="nav-link m-tabs__link" data-toggle="tab" href="#itemInfo" role="tab">
                                        <i class="flaticon-info"></i> Item Info
                                    </a>
                                </li>
                                <li class="nav-item m-tabs__item" style="display:none">
                                    <a class="nav-link m-tabs__link" id="recentlyAccessed" data-toggle="tab" href="#RecentlyAccesed" role="tab">
                                        <i class="flaticon-list-3"></i> Recently Accesed Account
                                    </a>
                                </li>
                                <li class="nav-item m-tabs__item">
                                    <a class="nav-link m-tabs__link active" id="sizeColor" data-toggle="tab" href="#SizeColor" role="tab">
                                        <i class="flaticon-list-3"></i> Size & Color
                                    </a>
                                </li>
                                <li class="nav-item m-tabs__item">
                                    <a class="nav-link m-tabs__link" id="ColorMapping" data-toggle="tab" href="#_ColorMapping" role="tab">
                                        <i class="flaticon-list-3"></i> Color Mapping with Size
                                    </a>
                                </li>
                            </ul>
                            <div class="m-portlet__body">
                                <div class="tab-content">
                                    <!--User Info START-->
                                    <div class="tab-pane" id="itemInfo" role="tabpanel">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group row">
                                                    <input type="text" asp-for="@Model.ItemMastersDetail.Id" id="Id" />
                                                    <input type="hidden" asp-for="@Model.taxInfoDetails.taxType" id="taxType" />
                                                    <input type="hidden" asp-for="@Model.taxInfoDetails.calcuationTaxtype" id="calcuationTaxtype" />
                                                    <label class="col-md-3 col-form-label">Is it a special day item ?<span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-input--solid clear-cls" asp-for="ItemMastersDetail.IsSpecialDay" name="IsSpecialDay" id="IsSpecialDay">
                                                            <option value="Yes">Yes</option>
                                                            <option value="No">No</option>
                                                        </select>
                                                        <span class="error font-small-3 error-empty-cls" id="spnIsspcialday" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <div class="form-group row" id="DaysCounter" style="display:none">
                                                    <label class="col-md-3 col-form-label">Choose the days available<span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-bootstrap-select m_selectpicker m-input--solid clear-cls" asp-for="@Model.GetDayList" name="AvailableDayS" id="AvailableDayS">
                                                            <option value="0">Sunday</option>
                                                            <option value="1">Monday</option>
                                                            <option value="2">Tuesday</option>
                                                            <option value="3">Wednesday</option>
                                                            <option value="4">Thursday</option>
                                                            <option value="5">Friday</option>
                                                            <option value="6">Saturday</option>
                                                            <option value="7">Everyday</option>
                                                        </select>
                                                        <span class="error font-small-3 error-empty-cls" id="spnAvailableday" style="display: none; color: red;"></span>
                                                    </div>
                                                    <input type="hidden" id="avaldayno" asp-for="ItemMastersDetail.AvailableDay" />
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Available during specific hours ?<span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-input--solid clear-cls" asp-for="ItemMastersDetail.MealTimeType" name="MealTimeType" id="MealTimeType">
                                                            <option value="Y">Yes</option>
                                                            <option value="N">No</option>
                                                        </select>
                                                        <input type="hidden" id="Mealtimetype" asp-for="ItemMastersDetail.MealTimeType" />
                                                        <span class="error font-small-3 error-empty-cls" id="spnMealtimetype" style="display: none; color: red;"></span>
                                                    </div>
                                                    <div class="Mealtimezone" style="display:none">
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control m-input m-input--solid clear-cls" name="StartTime" asp-for="ItemMastersDetail.StartTime" autocomplete="off" id="StartTime" placeholder="Start Time">
                                                            <span class="error font-small-3 error-empty-cls" id="spnStartTime" style="display: none; color: red;">This field is required</span>
                                                            <input type="text" asp-for="ItemMastersDetail.StartTime" id="StartTimeDb" hidden>

                                                        </div>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control m-input m-input--solid clear-cls" asp-for="ItemMastersDetail.EndTime" name="EndTime" autocomplete="off" id="EndTime" placeholder="End Time">
                                                            <span class="error font-small-3 error-empty-cls" id="spnEndTime" style="display: none; color: red;">This field is required</span>
                                                            <input type="text" asp-for="ItemMastersDetail.EndTime" id="EndTimeDb" hidden>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Printing this item in KOT slip? <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <select asp-for="ItemMastersDetail.KOT_Print" class="form-control m-input--solid clear-cls" name="KOT_Print" id="KOT_Print">
                                                            <option value="0" selected="selected" disabled>Select KOT Print</option>
                                                            <option value="1">Yes</option>
                                                            <option value="2">No</option>
                                                        </select>
                                                        <span class="error font-small-3" id="SpnkotPrint" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <hr />
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Item MainCategory <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <select asp-for="ItemMastersDetail.MainCategory" asp-items="Model.GetMainCategoryList" class="form-control m-input--solid clear-cls" name="MainCategory" id="MainCate"></select>
                                                        <span class="error font-small-3" id="SpnMainCate" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Item Category <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <select asp-for="ItemMastersDetail.Category" class="form-control m-input--solid clear-cls" name="Category" id="Category">
                                                            <option value="NA">Select Category</option>
                                                        </select>
                                                        <span class="error font-small-3" id="SpnCategory" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Item SubCategory <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <select asp-for="ItemMastersDetail.SubCategory" class="form-control m-input--solid clear-cls" name="SubCategory" id="SubCategory">
                                                            <option value="NA">Select SubCategory</option>
                                                        </select>
                                                        <span class="error font-small-3" id="Spnsubcategory" style="display: none; color: red;">This field is required</span>
                                                    </div>
                                                </div>
                                                <hr />
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Item Name<span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <input asp-for="ItemMastersDetail.ItemId" type="hidden" class="form-control m-input m-input--solid" id="ItemId" name="ItemId" placeholder="Enter Product Name">
                                                        <input asp-for="ItemMastersDetail.PluName" type="text" class="form-control m-input m-input--solid clear-cls" id="pluName" name="pluName" placeholder="Enter Product Name">
                                                        <span class="error font-small-3" id="Spnpluname" style="display: none; color: red;">This field is required</span>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Short Description <span class="text-warning"></span></label>
                                                    <div class="col-md-5">
                                                        <textarea asp-for="ItemMastersDetail.Description" rows="2" name="Description" id="Description" class="form-control m-input m-input--solid" style="resize:none;"></textarea>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Ingrendiant Used ? <span class="text-warning"></span></label>
                                                    <div class="col-md-5">
                                                        <textarea asp-for="ItemMastersDetail.LongDescription" rows="4" name="LongDescription" id="LongDescription" class="form-control m-input m-input--solid" style="resize:none;"></textarea>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Item Code / Barcode <span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <input asp-for="ItemMastersDetail.PluCode" type="text" class="form-control m-input m-input--solid clear-cls" id="PluCode" name="PluCode" placeholder="Enter Product Code">
                                                        <input asp-for="ItemMastersDetail.Category" type="hidden" class="form-control m-input m-input--solid" id="Cate" name="Cate" placeholder="Enter Product Code">
                                                        <input asp-for="ItemMastersDetail.SubCategory" type="hidden" class="form-control m-input m-input--solid" id="SubCate" name="SubCate" placeholder="Enter Product Code">
                                                        <input asp-for="ItemMastersDetail.PluCode" type="hidden" class="form-control m-input m-input--solid" id="PluCodehidden" name="PluCodehidden" placeholder="Enter Product Code">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Choose Variance Type: <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <select asp-for="ItemMastersDetail.Measurement" asp-items="Model.GetMeasurementList" class="form-control m-input--solid clear-cls" name="Measurement" id="Measurement">
                                                            <option value="0" selected="selected" disabled>Select Measurement</option>
                                                        </select>
                                                        <span class="error font-small-3" id="Spnmeasurement" style="display: none; color: red;">This field is required</span>

                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Gross Weight (in kg.) <span class="text-warning">*</span></label>
                                                    <div class="col-md-4">
                                                        <input asp-for="ItemMastersDetail.NetWeight" type="text" class="form-control m-input m-input--solid clear-cls" name="NetWeight" id="NetWeight" placeholder="eg. 0.5kg">
                                                    </div>
                                                </div>
                                                <hr />
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Food Type Indicator <span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-input--solid clear-cls" asp-for="ItemMastersDetail.FoodType" name="FoodType" id="FoodType" asp-items="Model.foodTypeList">
                                                            <option value="0" selected="selected">Select Food Type</option>
                                                        </select>
                                                        <span class="error font-small-3 error-empty-cls" id="SpnFoddType" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label"> Spicy Level ?<span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-input--solid clear-cls" asp-for="ItemMastersDetail.Spicy_Level" name="Spicy_Level" id="Spicy_Level">
                                                            <option value="0" selected="selected">Select Spicy Level</option>
                                                            <option value="1">Low</option>
                                                            <option value="2">Meduim</option>
                                                            <option value="3">Hot & Spicy</option>
                                                        </select>
                                                        <span class="error font-small-3 error-empty-cls" id="SpnSpicy_Level" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <label class="col-md-3 col-form-label">Preparation Time ?<span class="text-warning">*</span></label>
                                                    <div class="col-md-3">
                                                        <select class="form-control m-input--solid clear-cls" asp-for="ItemMastersDetail.PreparationTime" name="PreparationTime" id="PreparationTime">
                                                            <option value="0" selected="selected">Select Preparation Time</option>
                                                            <option value="1-5 mins">1-5 mins</option>
                                                            <option value="5-10 mins">5-10 mins</option>
                                                            <option value="10-15 mins">10-15 mins</option>
                                                            <option value="15-20 mins">15-20 mins</option>
                                                            <option value="20-30 mins">20-30 mins</option>
                                                            <option value="30-40 mins">30-40 mins</option>
                                                            <option value="40-1 hours">40-1 hours</option>
                                                            <option value="2 hours">2 hours</option>
                                                            <option value="3 hours">3 hours</option>
                                                            <option value="4 hous">4 hours</option>
                                                            <option value="5 hours">5 hours</option>
                                                            <option value="6 hours">6 hours</option>
                                                            <option value="7 hours">7 hours</option>
                                                            <option value="8 hours">8 hours</option>
                                                        </select>
                                                        <span class="error font-small-3 error-empty-cls" id="spnPreparationtime" style="display: none; color: red;"></span>
                                                    </div>
                                                </div>
