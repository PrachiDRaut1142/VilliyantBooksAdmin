    <style>
        .page-common-loader {
            position: fixed;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            z-index: 999;
            display: none;
            align-items: center;
            justify-content: center;
            background-color: rgb(0 0 0 / 70%);
        }

        .page-common-loader img {
            width: 4rem;
            object-fit: cover;
        }

        .page-common-loader.loader-show {
            display: flex;
        }

        .noti-content {
            position: absolute;
            right: 38px;
            background: #ffffff;
            border-radius: 4px;
            top: 65px;
            width: 250px;
            display: none;
            box-shadow: 0px 0px 15px 1px rgb(69 65 78 / 20%);
        }

        ul#notiContent {
            max-height: 200px;
            overflow: auto;
            padding: 0px;
            margin: 0px;
        }

        ul#notiContent li {
            margin: 3px;
            padding: 5px 8px;
            background: #fff;
            display: block;
        }

        .noti-top-arrow {
            border-color: transparent;
            border-bottom-color: #ffffff;
            border-style: dashed dashed solid;
            border-width: 0 8.5px 8.5px;
            position: absolute;
            right: 32px;
            top: -8px;
        }

        .notification-popup .noti {
            display: block;
            cursor: pointer;
        }

        .notification-popup .noti:hover i {
            color: #313131;
        }

        span.count {
            width: 21px;
            height: auto;
            background-color: red;
            align-self: center;
            text-align: center;
            border-radius: 20px;
            position: absolute;
            top: 23px;
            left: 14px;
        }

        .toast .toast-close-button:before {
        content:"X"!important;
        }

        .layoutBranch {
            position: absolute;
            top: 20px;
            width: 25%;
            left: 50%;
            transform: translateX(-50%);
            -webkit-transform: translateX(-50%);
        }
        .ViewAll{
            text-align: center;
            color: red;
            margin-block: 8px;
            margin-left: -68px;
        }
    </style>
