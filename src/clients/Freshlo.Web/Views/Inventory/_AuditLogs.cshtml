@using Freshlo.DomainEntities.Inventory;
@model List<InventoryAsset>


 <table class="table table-striped-even ab-table-head-bg-warning-light table-borderless table-middle m-data-table" id="zipcodelist2">
                                            <thead>
                                                <tr>
                                                    <th>AuditId</th>     
                                                    <th>Audited Date</th>                                                 
                                                    <th>Audited BY</th>
                                                    <th>Remarks</th>
                                                    <th>View</th>
                                                </tr>
                                            </thead>
                                            <tbody id="inventoryassetadhoc">
                                              @foreach (var item in Model)
    {
         <tr>  
            <td><span href="#" class="font-medium-1">@item.AuditId</span></td>
            <td><spam class="font-medium-1" >@item.CreatedDate</spam></td>
            <td><span class="font-medium-1">@item.CreatedBy</span></td>
            <td><span class="font-medium-1">@item.Remarks</span></td>
            <td>
            <a href="#" data-toggle="modal" class="text-dark" data-Id="@item.AuditId" id="auditlist"> <i class="m-menu__link-icon la la-eye"> </i> </a></td>
         </tr>                                           
    }
                                              
                                            </tbody>
                                        </table>


                                        
@section Scripts{
    <script src="~/assets/app/js/datatable.js"></script>
}
    
