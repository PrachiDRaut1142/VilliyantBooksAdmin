        display: flex;
        padding: 0;
        flex-wrap: wrap;
        margin-top: 0.5rem;
    }

        .payment_status li,
        .payment_mode li {
            list-style: none;
            display: inline-block;
            padding: 0.5rem 0;
            margin-right: 1rem;
            text-align: center;
            border: 2px dashed #c5c1c1;
            cursor: pointer;
            width: 30%;
            margin-bottom: 1rem;
        }

            .payment_status li i,
            .payment_mode li i {
                margin-right: 0.2rem;
                vertical-align: middle;
                display: none;
            }

            .payment_status li.active {
                border-color: #cc2424;
                color: #cc2424;
            }

            .payment_mode li.active {
                border-color: #cc2424;
                color: #cc2424;
